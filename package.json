    "deploy-basic-contract": "truffle migrate -f 2 --to 2  --compile-none",
    "deploy-basic-contract-idle": "truffle migrate -f 2 --to 2  --reset --compile-all --network idle",
    "deploy-basic-contract-angle": "truffle migrate -f 2 --to 2  --reset --compile-all --network angle",
    "deploy-local-idle": "truffle migrate -f 6 --to 6 --compile-none --network idle  && npm run deploy-idle-pools",
    "deploy-local-angle": "truffle migrate -f 7 --to 7 --compile-none --network angle  && npm run deploy-angle-pools",
    "deploy-idle-pools": "truffle migrate  -f 24 --to 24  --compile-none --network idle",
    "deploy-angle-pools": "truffle migrate  -f 25 --to 25  --compile-none --network angle",
    "deploy-local": "truffle migrate -f 6 --to 7 --compile-none && truffle migrate -f 24 --to 25 --compile-none",
    "deploy-server": "truffle migrate --network server_fork -f 2 --to 2  --compile-none && npm run deploy-server-pickle && npm run deploy-server-curve && npm run deploy-server-ribbon && npm run deploy-server-idle && npm run deploy-server-angle && npm run deploy-server-balancer",
    "deploy-server-idle": "truffle migrate --network server_fork -f 6 --to 6 --compile-none && truffle migrate --network server_fork -f 24 --to 24  --compile-none",
    "deploy-server-angle": "truffle migrate --network server_fork -f 7 --to 7 --compile-none && truffle migrate --network server_fork -f 25 --to 25  --compile-none",
    "test-all": "npm run deploy-local && truffle test --compile-none --migrations_directory test",
    "test-no-deploy": "truffle test --compile-none --migrations_directory test",
    "test-no-deploy-custom": "truffle test ./test/multiple_rewards.js --compile-none --migrations_directory test",
    "test-no-deploy-idle": "truffle test --compile-none --migrations_directory test --network idle",
    "test-no-deploy-angle": "truffle test --compile-none --migrations_directory test --network angle",
